{
    "name": "artisanry/exception-handlers",
    "description": "Exceptions Handlers for Laravel 5",
    "keywords": [
        "laravel",
        "framework",
        "Laravel-Exception-Handlers",
        "Laravel Exception Handlers"
    ],
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "homepage": "https://basecode.sh"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8",
        "illuminate/http": "^5.8"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2"
    },
    "suggest": {
        "raven/raven": "A PHP client for Sentry (http://getsentry.com). Require 0.12.0.",
        "bugsnag/bugsnag": "Official Bugsnag notifier for PHP applications. Require ~2.5.",
        "dbtlr/php-airbrake": "A PHP 5.3 library for sending errors to the Airbrake.io service. Require ~1.1."
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\ExceptionHandlers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\ExceptionHandlers\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
