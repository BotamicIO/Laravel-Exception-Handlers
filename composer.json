{
    "name": "faustbrian/laravel-exception-handlers",
    "description": "Exceptions Handlers for Laravel 5",
    "keywords": ["laravel", "framework", "Laravel-Exception-Handlers", "Laravel Exception Handlers", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.3.*",
        "illuminate/http": "5.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "suggest": {
        "raven/raven": "A PHP client for Sentry (http://getsentry.com). Require 0.12.0.",
        "bugsnag/bugsnag": "Official Bugsnag notifier for PHP applications. Require ~2.5.",
        "dbtlr/php-airbrake": "A PHP 5.3 library for sending errors to the Airbrake.io service. Require ~1.1."
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\ExceptionHandlers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\ExceptionHandlers\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
